<?xml version="1.0" encoding="UTF-8"?><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.espertech.com/schema/esper" xsi:schemaLocation="http://www.espertech.com/schema/esper http://www.espertech.com/schema/esper/esper-configuration-4-0.xsd">	<event-type-auto-name package-name="event"/>		<database-reference name="logcenter_builtin_db">        <drivermanager-connection class-name="org.postgresql.Driver" url="jdbc:postgresql://192.168.0.111:5432/logcenter?user=logcenter&amp;password=logcenter" user="logcenter" password="logcenter">            <connection-arg name="user" value ="logcenter"/>            <connection-arg name="password" value ="logcenter"/>        </drivermanager-connection>        <!-- <metadata-origin value="sample" /> -->        <lru-cache size="1000"/>        <expiry-time-cache max-age-seconds="60" purge-interval-seconds="120" ref-type="weak"/>        <connection-lifecycle value="retain"/>    </database-reference>      <engine-settings>        <defaults>            <execution prioritized="true"/>             <metrics-reporting enabled="true" engine-interval="1000" statementinterval="1000" threading="true"/>                          <threading>                <listener-dispatch preserve-order="true" timeout-msec="2000" locking="suspend"/>            </threading>            <logging>                <execution-path enabled="false"/>                <timer-debug enabled="false"/>            </logging>        </defaults>    </engine-settings>    <!--        <plugin-loader name="EsperJMX" class-name="com.espertech.esper.jmx.client.EsperJMXPlugin">        <init-arg name="use-platform-mbean-server" value="true" />        <init-arg name="rmi-registry-port" value="1099" />    </plugin-loader>      <esperio-http-configuration>          <service name="myservice" port="8877" nio="false"/>          <get service="myservice" pattern="*"/>     </esperio-http-configuration> --></esper-configuration>